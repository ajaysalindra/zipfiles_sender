<section class="container continer-responsive">
		<div class="panel-clr-new">
			<div class="panel-heading">
				<h1 align="center">Zip File Upload</h1>
				<div align="right"><%= link_to "Add Cyborgs", cyborgs_path, :class => "btn-s btn-xs-s" %></div>
			</div>
			<div class="jumbotron-new">
				    <table class="table">
						<% @uploadzipfiles.each do |uploadzipfile| %>
							<tr>
							<td><a class="text-clr-new" href="<%= uploadzipfile_path(uploadzipfile) %>"><%= uploadzipfile.zipfile_identifier.to_s %></a>
							</td>
							<td><%= link_to 'Delete', uploadzipfile_path(uploadzipfile), method: :delete, data: { confirm: 'Are you sure?' } %></td>
							</tr>
							<% end %>
					</table>
			
			<div><%= link_to "Upload Zip", new_uploadzipfile_path, :class => "btn-s btn-xs-s" %></div>
			</div>
		</div>
      
</section>